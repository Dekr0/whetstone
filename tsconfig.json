{
    "compilerOptions": {
        "outDir": "dist",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "target": "esnext",
        "module": "commonjs",
        "baseUrl": "./src",
        "resolveJsonModule": true,
        "paths": {
            "array-hashing/*": ["try-1/array-hashing/*"]
        }
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "node_modules",
        "./src/template/typescript/"
    ]
}
